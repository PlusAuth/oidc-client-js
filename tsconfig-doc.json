{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "allowUmdGlobalAccess": true,
    "allowJs": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "types",
    "sourceMap": true,
    "outDir": "lib",
    "strict": true,
    "useUnknownInCatchVariables": false,
    "removeComments": false,
    "noImplicitAny": false,
  },
  "typedocOptions": {
    "out": "./docs",
    "name": "PAOIDCClient",
    "readme": "none",
    "categorizeByGroup": false,
    "entryPoints": [
      "src/index.ts"
    ],
    "hideGenerator": true,
    "listInvalidSymbolLinks": false,
    "excludeProtected": true,
    "excludePrivate": true,
    "plugin": [
      "typedoc-plugin-merge-modules"
    ]
  }
}
